{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning",
            "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
        }
    },
    "ConnectionStrings": {
        /* The connection string for the database */
        "DefaultConnection": "Data Source=bsky.db"
    },
    "AtProto": {
        /* The url of the AtProto service */
        "ServiceUrl": "https://bsky.social",
        /* Your bluesky login ex. myaccount.bsky.social */
        "LoginIdentifier": "myaccount.bsky.social",
        /* Your bluesky password or https://bsky.app/settings/app-passwords */
        "LoginToken": "xxxx-xxxx-xxxx-xxxx"
    },
    "Feed": {
        /* The Did for your feed ex. did:web:{YOUR_DOMAIN} */
        "ServiceDid": "did:web:feeds.example.com",
        /* The Did for your bluesky profile ex. did:plc:{XXX} */
        /* You can find this at https://bsky.social/xrpc/com.atproto.identity.resolveHandle?handle={YOUR_BLUESKY_PROFILE} */
        "PublisherDid": "did:plc:xxxxxxxxxxxx",
        /* The domain you are hosting your feed on ex. example.com */
        "HostName": "feeds.example.com",
        /* 
            Mapping from internal feed name to atproto feed name 
            https://bsky.app/profile/{publisherDid}/feed/{atproto-feed-name}
            ex. https://bsky.app/profile/did:plc:xxxxxxxxxxxx/feed/test-feed
        */
        "Feeds": {
          "myposts": "test-feed",
          "ffxiv": "final-fantasy-xiv",
          "linux": "linux"
        }
    }
}